{
  "name": "[DEV] Range Sync",
  "version": "1.0",
  "description": "[DEV] The Range Chrome extension, rearchitected to be lighter-weight and flexible",
  "permissions": ["tabs", "cookies", "storage", "https://*.habdev.site/*"],
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "background/api.js",
      "background/conf/dev.js",
      "background/background.js",
      "background/filter/filter.js",
      "background/filter/asana.js",
      "background/filter/confluence.js",
      "background/filter/dropbox_paper.js",
      "background/filter/figma.js",
      "background/filter/github.js",
      "background/filter/gmail.js",
      "background/filter/google_drive.js",
      "background/filter/lever.js",
      "background/filter/lucid_chart.js",
      "background/filter/medium.js",
      "background/filter/metabase.js",
      "background/filter/quip.js",
      "background/filter/stack_overflow.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
