{
  "name": "Range Sync",
  "version": "1.0",
  "description": "The Range Chrome extension, rearchitected to be lighter-weight and flexible",
  "permissions": ["tabs", "cookies", "https://*.range.co/*"],
  "options_page": "options/options.html",
  "background": {
    "scripts": [
      "background/conf/prod.js",
      "background/api.js",
      "background/background.js",
      "background/regex.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
